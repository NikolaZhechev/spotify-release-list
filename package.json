    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pretty-bytes": "^6.1.1",
    "query-string": "^9.1.1",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dates": "^21.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-hotkeys-hook": "^4.6.1",
    "react-intersection-observer": "^9.13.1",
    "react-ios-pwa-prompt": "^2.0.6",
